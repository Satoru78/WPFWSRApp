<Window x:Class="WPFWSRApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFWSRApp"
        mc:Ignorable="d"
        Title="Стражник" Height="500" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Grid.Row="0" VerticalAlignment="Top" HorizontalAlignment="Right">
            <TextBlock Name="tblNameUser" Margin="10" FontSize="14"></TextBlock>
        </StackPanel>

        <Frame Grid.Row="1" Name="MainFrame" NavigationUIVisibility="Hidden">

        </Frame>
    </Grid>
</Window>
